{
  "extends": [
    "stylelint-config-standard-scss",
    "stylelint-config-clean-order",
    "stylelint-config-prettier"
  ],
  "plugins": [
    "stylelint-order"
  ],
  "rules": {
    "order/order": [
      "custom-properties",
      "dollar-variables",
      {
        "type": "at-rule",
        "name": "include",
        "hasBlock": false
      },
      "declarations",
      {
        "type": "at-rule",
        "name": "include",
        "hasBlock": true
      },
      {
        "type": "rule",
        "selector": "&::before"
      },
      {
        "type": "rule",
        "selector": "&::after"
      },
      "rules",
      {
        "type": "rule",
        "selector": "&:first-child"
      },
      {
        "type": "rule",
        "selector": "&:last-child"
      },
      {
        "type": "rule",
        "selector": "&:nth-child.+"
      },
      {
        "type": "rule",
        "selector": "&:link"
      },
      {
        "type": "rule",
        "selector": "&:hover"
      },
      {
        "type": "rule",
        "selector": "&:focus"
      },
      {
        "type": "rule",
        "selector": "&:visited"
      },
      {
        "type": "rule",
        "selector": "&:active"
      },
      {
        "type": "rule",
        "selector": "&:disabled"
      },
      {
        "type": "rule",
        "selector": "&\\[[^\\[\\]]+\\]"
      },
      {
        "type": "rule",
        "selector": "&--.+"
      },
      {
        "type": "rule",
        "selector": "&\\..+"
      },
      {
        "type": "rule",
        "selector": ".* &"
      },
      {
        "type": "rule",
        "selector": "&__.+"
      },
      "at-rules"
    ],
    "selector-class-pattern": null,
    "function-url-quotes": null,
    "no-descending-specificity": null
  }
}